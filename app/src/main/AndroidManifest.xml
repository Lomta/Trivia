<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.fridge">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Fridge">
        <activity android:name=".MyProfile"></activity>
        <activity
            android:name=".ProducersActivity"
            android:label="@string/title_activity_producers"
            android:theme="@style/Theme.Fridge.NoActionBar" />
        <activity
            android:name=".ProducerActivity2"
            android:label="@string/title_activity_producer2"
            android:theme="@style/Theme.Fridge.NoActionBar" />
        <activity
            android:name=".MenuActivity"
            android:label="@string/title_activity_menu"
            android:theme="@style/Theme.Fridge.NoActionBar" />
        <activity android:name=".EventsFeed" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>